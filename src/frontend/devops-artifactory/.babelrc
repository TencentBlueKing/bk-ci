{
    "presets": [
        [
            "env",
            {
                "modules": "commonjs",
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
                },
                "debug": false,
                "useBuiltIns": true
            }
        ],
        "stage-2"
    ],
    "plugins": ["transform-runtime", "transform-object-assign", "syntax-dynamic-import", "transform-vue-jsx", "transform-class-properties", "transform-decorators-legacy"],
    "env": {
        "test": {
            "presets": ["env", "stage-2"]
        }
    }
}